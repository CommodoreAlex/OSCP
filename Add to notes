nmap -sV -Pn -vv -p 445 --script='(smb*) and not (brute or broadcast or dos or external or fuzzer)' --script-args=unsafe=1 $ip

nmap -p 1433 --script ms-sql-info,ms-sql-empty-password,ms-sql-xp-cmdshell,ms-sql-config,ms-sql-ntlm-info,ms-sql-tables,ms-sql-hasdbaccess,ms-sql-dac,ms-sql-dump-hashes  --script-args mssql.instance-port=1433,mssql.username=sa,mssql.password=,mssql.instance-name=MSSQLSERVER $ip

# Enumerate for files with 404 results ommitted: /FUZZ for files
wfuzz -c -z file,/usr/share/wordlists/Seclists/Discovery/Web-Content/raft-large-files.txt --hc 404 http://192.168.1.1/FUZZ
# Enumerate for directores /FUZZ/ for directories
wfuzz -c -z file,/usr/share/wordlists/Seclists/Discovery/Web-Content/raft-large-directories.txt --hc 404 http://192.168.1.1/FUZZ/

# We can test to see if we have command execution when there is no output by using ping
ping -c 3 $ip # in the non-verbose command shell
# Look for ICMP requests on our end
tcpdump -i any "icmp"

# Privilege Escalation
# When on the box you can type 'which mysql' and then try default credentials
mysql -uroot -p # unauthenticated login on the localhost
